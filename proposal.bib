  booktitle={International Workshop on the Arithmetic of Finite Fields},
  pages={189--201},
  year={2007},
  abstract={This paper proposed addition formula through using Euclidean addition chains EAC with Fibonacci to avoid difficult to find small chain. Author uses Fibonacci-and-add algorithm instead of double and add (with using Jacobian coordinate and characteristic greater than 3). He added improvements on his scheme through window and signed representation to improve algorithm performance. Author compared his scheme with many schemes (D \& A, NAF, 4-NAF and Montgomery ladder). He found his scheme outperform on D \& A only in the case improvement addition (window or signed representation)},
  organization={Springer}
}

@inproceedings{p65,
  title={Software implementation of elliptic curve cryptography over binary fields},
  author={Hankerson, Darrel and Hernandez, Julio L{\'o}pez and Menezes, Alfred},
  booktitle={International Workshop on Cryptographic Hardware and Embedded Systems},
  pages={1--24},
  year={2000},
  abstract={This paper presents implementation of ECC and ECDSA algorithm on random binary and Koblitz curves ($F_{2^{163}}, F_{2^{233}} ,F_{2^{283}}$) for NIST's standards. Authors use projective coordinate instead of Affine that lead to improve significant in PM. Through implementation, they found the best algorithm in term timing in field multiplication that is LR Comb with w=4 compared with shift-and-add, LR Comb, RL Comb and Karatsuba algorithm. While the best algorithm in inversion is extended Euclidean algorithm EEA compared with almost inverse algorithm AIA and modified almost inverse algorithm MAIA when implementation in C on Pentium II. Also they noted the best algorithm for PM in random curves (ECC and ECDSA) is fixed-base comb with w=4, while in Koblitz curves is fixed-base window TNAF with w=6 in the case memory is not constrained. When memory constrained, Montgomery is the best with random and TNAF is the best with Koblitz. They point out Koblitz curves are faster than random binary curves.},
  organization={Springer}
}

@inproceedings{p66,
  title={An analysis of double base number systems and a sublinear scalar multiplication algorithm},
  author={Ciet, Mathieu and Sica, Francesco},
  booktitle={International Conference on Cryptology in Malaysia},
  pages={171--182},
  year={2005},
  abstract={This paper analyzes scalar in DBNS algorithm on superlinear EC in characteristic 3. Authors proposed sublinear SM algorithm for PM (i.e. sublinear in scalar's length) with running time O($\frac{log\  n}{log\ log\ n}$), and it is faster than D \& A and T \& A. Authors point out that their algorithm is appropriate to use large parameters in EC, where selecting large parameters leads to improve performance and security},
  organization={Springer}
}

@inproceedings{p67,
  title={Fast elliptic curve arithmetic and improved Weil pairing evaluation},
  author={Eisentr{\"a}ger, Kirsten and Lauter, Kristin and Montgomery, Peter L},
  booktitle={Cryptographersâ€™ Track at the RSA Conference},
  pages={343--354},
  year={2003},
  abstract={This paper presents a method to improve PM's performance with Affine coordinate in ECC. This method eliminates field multiplication in the case using LR binary SM. Which it eliminates y-coordinate in addition, doubling and tripling operations when getting result point. This scheme leads to save field multiplication and thus improvement of PM's cost from 3.8\% to 8.5\%. To find operation of 2P+Q, Authors used form ((P+Q)+P), also operation of 3P+Q, they used (((P+Q)+P)+P). This trick leaded to improve PM. For example, when number of points (1133044P), cost of original PM is 23div+41s+23m, while cost of improved PM is 23div+37s+19m, where there is improvement in m and s. In addition, authors proposed improvement on multiple PM and performance of Weil and Tate pairings (to 7.8\%).},
  organization={Springer}
}

@inproceedings{p68,
  title={A parallel architecture for fast computation of elliptic curve scalar multiplication over $GF (2^m)$},
  author={Saqib, Nazar A and Rodriguez-Henriquez, Francisco and Diaz-Perez, Arturo},
  booktitle={Parallel and Distributed Processing Symposium, 2004. Proceedings. 18th International},
  pages={144},
  year={2004},
  abstract={This paper presents a parallel architecture to accelerate PM in ECC with $GF(2^{191})$ on Xilinx VirtexE XCV3200 FPG device. Authors suggested using Montgomery algorithm depend on parallel-sequential manner. Their scheme improved PM's performance to $56.44\mu s$. They have compared their scheme with previous schemes (different H/W devices), results refer to this scheme presents better performance. Also authors pointed out their scheme balance between space and time},
  organization={IEEE}
}

@inproceedings{p69,
  title={Faster point multiplication on elliptic curves with efficient endomorphisms},
  author={Gallant, Robert P and Lambert, Robert J and Vanstone, Scott A},
  booktitle={Annual International Cryptology Conference},
  pages={190--200},
  year={2001},
  abstract={This paper proposed a method to improve PM's fast through using Endomorphisms with prime fiels. Authors combined simultaneous multiple point multiplication with left-to-right window-NAF, where they convert form (kp) for PM to form $k_1p+k_2\phi(p)$($\phi$ is Endomorphism operation). This method improved point doubling (79 point) and point addition (38 point) that lead to improve PM to 50\% better than traditional methods with $F_{p160}$. They presented many recommendations to protect many attacks such as Pohlig-Hellman, Pollard's rho, Semaev-Satoh-Smart, Weil pairing and Tate pairing through $n \geq 2^{160}$, $ \# E(F_q)\neq q$ and n is not dividable on $q^i-1$ when $1 \leq i \leq 20$},
  organization={Springer}
}


@article{p70,
