@article{p63,
  title={Point multiplication on ordinary elliptic curves over fields of characteristic three},
  author={Smart, Nigel P and Westwood, EJ},
  journal={Applicable Algebra in Engineering, Communication and Computing},
  volume={13},
  number={6},
  pages={485--497},
  year={2003},
  abstract={This paper investigates using characteristic 3 with both Weiestrass and Hessian forms. Authors pointed out that characteristic 3 is efficient in Weiestrass form, where tripling operation performs more efficient than double and add, while characteristic 3 is not efficient with Hessian form (Triple and add used with Weistrass and double and add used with Hessian). They used different coordinate systems (Affine, Projective, Jacobian and Lopez Dahab Projective) on point addition and doubling. They note Jacobian is the most efficient with Weiestrass while Projective is the most efficient with Hessian. They also used mixed coordinate (Affine-Projective) on both Weistrass and Hessian. Through using mixed coordinate, results refer to improve timing in addition formula},
