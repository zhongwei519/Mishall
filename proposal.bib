<span id="LC1480" class="line" lang="plaintext">@inproceedings{p78,</span>
<span id="LC1481" class="line" lang="plaintext">  title={An ECDSA processor for RFID authentication},</span>
<span id="LC1482" class="line" lang="plaintext">  author={Hutter, Michael and Feldhofer, Martin and Plos, Thomas},</span>
