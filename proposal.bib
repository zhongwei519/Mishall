@article{p53,
  title={Trading inversions for multiplications in elliptic curve cryptography},
  author={Ciet, Mathieu and Joye, Marc and Lauter, Kristin and Montgomery, Peter L},
  journal={Designs, codes and cryptography},
  volume={39},
  number={2},
  pages={189--206},
  year={2006},
  abstract={This paper proposed point tripling (3P) and mix it with point doubling (2P) depend on various methods to evaluate 2P+Q when 1 inversion is more than 6 multiplications that lead to improve tripling. It presents comprehensive evaluation through i, s and m for operations types: P+Q, 2P, 2P+Q, 3P, 3P+Q, 4P, 4P+Q. Authors note that 2P+Q is faster than P+(P+Q) but 2P+Q is slightly more cost. Authors use Eisentrager  et al.'s idea in remove y3 from equations when compute 2P+Q, Authors use this idea with 3P+Q and remove y4 when 1i is more than 6m to reduce computation cost. Results prove this scheme improve SM efficient in ECC, ECDSA and ECDH},
  publisher={Springer}
}

@inproceedings{p54,
  title={Efficient and secure elliptic curve point multiplication using double-base chains},
  author={Dimitrov, Vassil and Imbert, Laurent and Mishra, Pradeep Kumar},
  booktitle={International Conference on the Theory and Application of Cryptology and Information Security},
  pages={59--78},
  year={2005},
  abstract={This paper proposed double-base chains method through prime and binary fields, where using point tripling with Jacobian coordinate in prime fields, and quadrupling combined with quadruple-and-add with affine coordinate in binary fields. Authors use modified greedy algorithm to convert k to DBNS form. They use the same idea that used it Eisentrager et al. to evaluate only x in compute 2P+Q but with 4P+Q to increase SM efficient. Through results, authors get efficient in speeding SM better than classical D \& A (21\%), NAF (13.5\%) and ternary/binary (5.4\%) in binary fields. In prime fields, this scheme also get results better than D \& A (25.8\%), NAF (15.8\%), 4NAF (6\%) and others. But results in \cite{p53} is better than this scheme in term inversion in the computation case 4P and 4P+Q. Also this scheme use protection mechanisms against SCA attack (SPA and DPA), where used side channel atomicity against SPA and randomization method against DPA},
  organization={Springer}
}

