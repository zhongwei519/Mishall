@inproceedings{p55,
  title={Efficient quintuple formulas for elliptic curves and efficient scalar multiplication using multibase number representation},
  author={Mishra, Pradeep Kumar and Dimitrov, Vassil},
  booktitle={International Conference on Information Security},
  pages={390--406},
  year={2007},
  abstract={This paper presented a method to improve SM through using quintupling (5P). Authors use 3 bases (2,3,5) or called SMBR. They use this scheme with affine in binary field and Jacobian in prime field similarly the scheme in \cite{p54}; therefore, this scheme is generalization for \cite{p54} but with 3 bases. Multibase representation algorithms are shorter, more redundant and more sparseness than DBNS algorithms. For instance, representation 160 bit costs 23 terms in bases (2,3) while 15 terms in bases (2,3,5). Authors improved on greedy algorithm (mgreedy) in order to fit SMBR in addition to shorter representation and faster running. They recommended to use small values for exponent because it is not effect on cost. Computation of 5P efficient through 2(2P)+P and 2P+3P, they found that 2(2P)+P costs 9s+17m (with affine ) and 14s+20m (with Jacobian) while 2P+3P costs 22m+s (with affine) and 26m+16s (with Jacobian). Through results, authors get efficient in SM with prime and binary fields better than previous algorithms such as binary, NAF, 3-NAF,4-NAF, ternary/binary and DB chains whether with precomputation or without precomputation points},
  organization={Springer}
}

@inproceedings{p56,
