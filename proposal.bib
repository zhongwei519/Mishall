@article{p7,
  title={Quark: A lightweight hash},
  author={Aumasson, Jean-Philippe and Henzen, Luca and Meier, Willi and Naya-Plasencia, Mar{\'\i}a},
  journal={Journal of cryptology},
  volume={26},
  number={2},
  pages={313--339},
  year={2013},
  abstract={This paper discussed the design of light Quark algorithm with three cases is the u-quark (136 bit), d-quark (176 bit) and s-quark (256 bit). This algorithm uses shift register instead of the s-box in Spongent and Photon algorithms and is inspired by the stream cipher Grain and block cipher KATAN algorithms. It also uses sponge construction (to improve operation) and core permutation. Researchers pointed out that there are devices constrained source (such as RFID) uses little power at the same time, these devices need protection from attacks. Results discussed by researchers that this algorithm implements short light calculations, leading to reduced time (reduction of consumption energy) to generate keys in addition to the quark algorithm's ability to repel the attacks},
  publisher={Springer}
}

@inproceedings{p8,
  title={A Key Agreement Algorithm Based on ECDSA for Wireless Sensor Network},
  author={Singh, Akansha and Awasthi, Amit K and Singh, Karan},
  booktitle={Proceedings of 3rd International Conference on Advanced Computing, Networking and Informatics},
  pages={143--149},
  year={2016},
  abstract={The idea of this research is to use a few of the keys to save energy, memory and reduce the computation. Researchers used a new scheme of ECDSA to secure the connection between the gateway and the cluster head(CH), and CH and nodes. This scheme broadcasts only session keys (km), which reduces energy as the session keys deletes after their account and that lead to less memory (the number of a few keys stored in memory). The public key of the gateway does not require power, and calculation processes accomplished through the use of a random number and hash function that require less calculated. This protocol uses periodic authentication in session key, it repels attacks and appropriate for networks of constrained-source},
  organization={Springer}
}

@inproceedings{p9,
  title={Analyzing energy and time overhead of security mechanisms in wireless sensor networks},
  author={Trakadas, P and Zahariadis, T and Leligou, HC and Voliotis, S and Papadopoulos, K},
  booktitle={2008 15th International Conference on Systems, Signals and Image Processing},
  pages={137--140},
  year={2008},
  abstract={This research analyzes the ECC algorithm (SHA-1) and RSA (AES) in several types of sensor nodes (Mica2dot, Mica2, Micaz and TelosB) in terms of energy and time. It also included many studies, all of which proved that the ECC is the best in terms of time and energy from the RSA, as ECC accomplish the same security but ECDSA using shorter key (160 bit), which reduces memory, computation and energy. Therefore, the authors concluded that the ECC better in the time and energy of the RSA and that the cost of data transfer is higher than the cost of computation as the data size transmitted less in ECDSA or ECC},
  organization={IEEE}
}

@inproceedings{p10,
  title={Implementation of ECDSA in WSN},
  author={Kodali, Ravi Kishore},
  booktitle={Control Communication and Computing (ICCC), 2013 International Conference on},
  pages={310--314},
  year={2013},
  abstract={Researcher presented implementation of ECDSA in sensor node (IRIS), but because this node supports 8 bits of the microcontroller the researcher modified the SHA-1 code from 32 bits original to 8 bits. When the researcher implemented this algorithm note that the original algorithm is best in size and the time of modified algorithm. But the researcher proved to be possible to use ECDSA algorithm with the sensor node held 8bits microcontroller. Therefore, small signatures used with ECDSA in IRIS node},
  organization={IEEE}
}

@article{p11,
  title={A Novel Approach for Secure Data Aggregation in Wireless Sensor Networks},
  author={Jariwala, Vivaksha and Jinwala, Devesh},
  journal={arXiv preprint arXiv:1203.4698},
  abstract={In this paper, the author used EC-OU algorithm for encryption and ECDSA for integration, as it focused on the process of data collection in the intermediate nodes, it used homomorphism property, which means direct computation on encrypted data. Each node in the network contains a public key, signed and encrypted message. Decryption and verification of signed messages only be in the BS. Researchers demonstrated that communication takes more energy than processing the data, so the data collection reduces transferring and thus reduces energy consumption},
  year={2012}
}

@article{p12,
  title={An Improved ECDSA Scheme for Wireless Sensor Network},
  author={Zhong, Hong and Zhao, Rongwen and Cui, Jie and Jiang, Xinghe and Gao, Jing},
  journal={International Journal of Future Generation Communication and Networking},
  volume={9},
  number={2},
  pages={73--82},
  year={2016},
  abstract={Researchers investigated ECDSA algorithm in detail, and found that it contains some of the problems that make it inefficient because of the inversion processes used to generate and verify the signature. Inversion process consumes a great time, which affects the calculation, this process consumes 10 times to multiplication process. In this research inversion process removed and the results demonstrate that this scheme is more efficient than the previous scheme where it had less time and therefore less calculated and energy. This scheme was applied to the sensor node Micaz},
  publisher={SCIENCE \& ENGINEERING RESEARCH SUPPORT SOC RM 402, MAN-JE BLDG, 449-8 OJUNG-DONG, DAEDOEK-GU, DAEJON, 00000, SOUTH KOREA}
}

@article{p13,
  title={Survey of security technologies on wireless sensor networks},
  author={Yang, Qiuwei and Zhu, Xiaogang and Fu, Hongjuan and Che, Xiqiang},
  journal={Journal of Sensors},
  volume={2015},
  year={2015},
  abstract={This paper presented a study on security techniques in WSN, where it focused on three features in WSN security that is the key management, authentication, and secure routing. Researchers discussed every feature in detail, as presented previous studies, protocols and the pros and cons of each protocol in each feature},
  publisher={Hindawi Publishing Corporation}
}

@inproceedings{p14,
  title={Implementation of ECC/ECDSA cryptography algorithms based on Java card},
  author={Han, Jin-Hee and Kim, Young-Jin and Jun, Sung-Ik and Chung, Kyo-Il and Seo, Chang-Ho},
  booktitle={Distributed Computing Systems Workshops, 2002. Proceedings. 22nd International Conference on},
  pages={272--276},
  year={2002},
  abstract={This paper investigated the design and implementation of ECC and ECDSA algorithms and use it in constrained-source devices like smart cards. Authors used java card that supports the java language and the environment used is NGICC, they found by the results that the ECC better than RSA because they use fewer gates, in addition noticed processing time and memory in the ECC is less than RSA, also signature generation in ECDSA is less time from signature verification. Authors concluded that the ECC and ECDSA are very useful for the smart cards because these algorithms provide appropriate security for constrained-resource devices. Also, this paper pointed to the possibility using of these algorithms in the other wireless devices constrained-source},
  organization={IEEE}
}

@inproceedings{p15,
  title={An efficient secure data aggregation scheme for wireless sensor networks},
  author={Othman, Soufiene Ben and Alzaid, Hani and Trad, Abdelbasset and Youssef, Habib},
  booktitle={Information, Intelligence, Systems and Applications (IISA), 2013 Fourth International Conference on},
  pages={1--4},
  year={2013},
  abstract={Authors used two algorithms for encryption and signing of the data collected, where they used encryption algorithm EC-OU to maintain confidentiality and ECDSA algorithm to maintain integration, both algorithms used during homomorphic. The idea in this scheme is that encryptions, signatures and public key gathered together from all nodes and send aggregator (CH), then these aggregators send the top level (parent aggregators) and so on up BS, that is, it is not the decryption or signature verification in the aggregators, but these processes are carried only in BS that has a high capacity. Authors concluded that this scheme offers energy-efficient and time due to collection of encryptions and signatures and sent to the BS as the signature verification process that takes more time than the signature generation, and the scheme efficient for large networks},
  organization={IEEE}
}

@inproceedings{p16,
  title={On the energy cost of communication and cryptography in wireless sensor networks},
  author={De Meulenaer, Giacomo and Gosset, Fran{\c{c}}ois and Standaert, Fran{\c{c}}ois-Xavier and Pereira, Olivier},
  booktitle={2008 IEEE International Conference on Wireless and Mobile Computing, Networking and Communications},
  pages={580--585},
  year={2008},
  abstract={This paper discussed the cost evaluation of energy (communication, computation) on the WSN through encryption protocols. Protocols that have been evaluated are the Kerberos key distribution protocol (symmetric encryption) ECDH-ECDSA key agreement protocol (asymmetric encryption). In this paper considered listening cost (i.e. using of LPL listening protocol where listening can be less by increasing sleep duration) which take the high cost of energy, this cost is not referred to in the previous studies. Authors have found that the cost of energy consumption in the Kerberos least one order of ECDH-ECDSA. They applied these protocols on Micaz and TelosB, and noted that TelosB sensor node consumes less power and also this has been proved by previous studies. Therefore, Hardware has a significant impact on energy. In addition, this paper conducted comparisons with previous research and found that the results this scheme more realistic and better. It also introduced a methodology process to evaluate the true cost of encryption in WSN, and indicate a preference ECDH-ECDSA on Kerberos because Kerberos requires a third party to perform key exchange while ECDH-ECDSA does not need},
  organization={IEEE}
}

@article{p17,
  title={Enhancing Energy Efficiency in Wireless Sensor Networks via Improving Elliptic Curve Digital Signature Algorithm},
  author={Kheradmand, Behbod and Mohammadkhanli, L},
  journal={World Applied Sciences Journal},
  volume={21},
  number={11},
  pages={1616--1620},
  year={2013},
  abstract={This paper focuses on reducing energy consumption in signature verification in ECDSA algorithm because verification signing spend more energy than the signature generation. This scheme depends on reducing the consumption of signature verification by using the concept of neighboring nodes, and thereby reduce the energy consumption of the network. It describes the original ECDSA with 2 scalar and addition point while the modified ECDSA scheme used 1 scalar and addition point with a 4 * 4 grid-based WSN. The idea in this paper is that the nodes send the results of intermediate computation to their neighbors, for example node A sends a result of intermediate calculation (signature verification N1P) , Signature and the message to node B, and B achieve one scalar and addition point to produce N2Q, where B produces N1P + N2Q and send the result to their neighbors, this will reduce energy consumption in  signature verification, as the proposed scheme faster 15.5\% and less energy  50\% of the original scheme. Also, this paper neglects the transmission and receiving energy, that it processes only energy of signature verification. The scheme applied to node Micaz Motes and simulation in MATLAB program},
  publisher={Citeseer}
}

@inproceedings{p18,
  title={The impact of ECC's scalar multiplication on wireless sensor networks},
  author={Rafik, Merad Boudia Omar and Mohammed, Feham},
  booktitle={Programming and Systems (ISPS), 2013 11th International Symposium on},
  pages={17--23},
  year={2013},
  abstract={This paper investigated study the execution time in the ECC algorithm (or ECDSA or ECD), If a large execution time, the energy consumption increases. Authors focused on the SCA attack used SPA and DPA analysis (DPA analysis neglected because it is not viable in the ECC algorithm). SCA attack is working on the use of information leaked from the SPM (Q = KP) and try to use this information to get the key. They also analyzed in detail the types of SPM algorithms that are D \& A, WM and CM. In addition to discussing the use of different coordinate systems such as projective coordinates and Affine coordinates, through experiments they found that coordinates projective faster by 91\% of Affine coordinates and vice versa for the memory, which the projective coordinates require more memory. During the results the authors concluded that the way CM faster than D \& A and WM because the CM uses less doubling and adding points. But this method need more memory. As for security, the D \& A method is the best with 27\% . D \& A is good at memory requirements but they need more time. Authors concluded several concepts that SPM is based on the user's application and constrained sources, execution time affects the energy consumption and protect the ephemeral K very important in ECDSA against SPA attacks. Authors carried out the Secure-CM and earned good time to implement (1.57 s), as demonstrated ECC is applicable in WSN},
  organization={IEEE}
}

