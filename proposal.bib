  booktitle={Cryptographersâ€™ Track at the RSA Conference},
  pages={250--265},
  year={2001},
  abstract={This paper presents a detailed study on implementation of ECC and ECDSA with prime field using NIST's standards. Authors explained field arithmetic, coordinate systems and methods of PM. They compare prime field with binary field. Through results, random prime curves and binary Koblitz are faster running time in PM when use of small fields in ECDSA's operations than random binary curves. Also, random prime curves are faster than binary Koblitz and random binary curves in the case signature verification of ECDSA (kP+lQ) when P is known in advance. C and assembler on a Pentium II 400MHz used through their implementation},
  organization={Springer}
}

@article{p60,
  title={Efficient elliptic curve exponentiation},
  author={Miyaji, Atsuko and Ono, Takatoshi and Cohen, Henri},
  journal={Information and Communications Security},
  pages={282--290},
  year={1997},
  abstract={This paper presents a study on efficient of EC exponentiation with prime field. EC exponentiation classified to three factors are coordinate, exponentiation for fixed point and exponentiation for random point. Authors used Jacobian coordinate to improve running time in EC exponentiation with FP and RP. Jacobian coordinate provides faster computation in doubling and slower computation in addition than projective coordinate. This paper focused on reducing point addition because it consumes larger cost than point doubling. When using Jacobian with fixed point, they improved PM by 4\% (when using of suited coefficient (a=0) improved arrived to 13\%). Authors combined NAF with window through random point with Jacobian coordinate to get improvement 33.7 addition and 157.9 doubling},
  publisher={Springer}
}

@inproceedings{p61,
  title={Efficient elliptic curve exponentiation using mixed coordinates},
  author={Cohen, Henri and Miyaji, Atsuko and Ono, Takatoshi},
  booktitle={International Conference on the Theory and Application of Cryptology and Information Security},
  pages={51--65},
  year={1998},
  abstract={This paper presented modified Jacobian coordinate system to get better performance than Affine, Projective, Jacobian and Chudnovsky Jacobian coordinate system (Affine has addition cost (I+2M+S) and doubling cost(I+2M+2S), Projective has addition cost (12M+2S) and doubling cost(7M+5S), Jacobian has addition cost (12M+4S) and doubling cost (4M+6S), Chudnovsky Jacobian has addition cost(11M+3S) and doubling cost (5M+6S) and, modified Jacobian has addition cost (13M+6S) and doubling cost (4M+4S). This coordinate system applied on EC exponentiation through prime field ($F_{p160}, F_{p192}$ and $F_{p224}$). Modified Jacobian is faster point doubling than all previous coordinate system. Authors point out that modified Jacobian mixed with other coordinate systems (modified Jacobian, Jacobian and Affine or modified Jacobian, Jacobian and Chudnovsky Jacobian) presents significant improvement on computation time with prime field ($F_{p160}, F_{p192}$ and $F_{p224}$)},
  organization={Springer}
}

@inproceedings{p62,
  title={Fast point multiplication on elliptic curves without precomputation},
  author={Joye, Marc},
  booktitle={International Workshop on the Arithmetic of Finite Fields},
  pages={36--46},
  year={2008},
  abstract={This paper presented NAF algorithm (right-to-left binary method) without precomputations (to use in constrained devices (RFID)). In addition, Author uses mixed coordinate with NAF algorithm, where uses Jacobian with point addition and modified Jacobian with point doubling. Author points out modified Jacobian is fast in point doubling but it is slow in point addition; therefore, author uses Jacobian in point addition. Through results, this scheme presents efficiency of scalar by 13.33$\ell$M (where $\ell$ is bits' length in scalar) compared with Jacobian (15.33$\ell$M) and modified Jacobian (14.33$\ell$M). But when three is more three field (temporary) variables are used, then left-to-right binary method described in [6] is better performance because it saves 1M+1S. Author investigates security in his algorithm. Where his algorithm protected against SPA and DPA attacks through using atomicity technique and classical countermeasures respectively. Also, this algorithm prevents doubling attacks},
  organization={Springer}
}

