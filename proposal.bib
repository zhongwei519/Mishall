  author={Hedabou, Mustapha and Pinel, Pierre and B{\'e}n{\'e}teau, Lucien},
  booktitle={International Conference on Information Security Practice and Experience},
  pages={85--96},
  year={2005},
  abstract={This paper presented improvement on fixed-base comb method against SCA attacks. Authors used sequence non-zero of bit-strings (i.e. removing all zeros only using 1 and -1) to represent ephemeral key. This idea leaded to prevent SPA. As They used randomized projective coordinates with each point precomputed to prevent DPA attacks. Using randomness for intermediate values and special points through using randomized linearly-transformed coordinates (RLC) is to prevent RPA and ZPA attacks. They proposed using randomized initial point method (RIP) to prevent SPA and DPA attacks where RIP uses random point $(R)$ with point multiplication $(kP)$. They pointed out that their scheme is a little more cost of computation time than original comb method},
  organization={Springer}
}

@inproceedings{p85,
