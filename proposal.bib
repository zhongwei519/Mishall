@inproceedings{p1,
  title={Random key predistribution schemes for sensor networks},
  author={Chan, Haowen and Perrig, Adrian and Song, Dawn},
  booktitle={Security and Privacy, 2003. Proceedings. 2003 Symposium on},
  pages={197--213},
  year={2003},
  abstract={In this paper suggests three mechanisms for the exchange of random key pre-distribution, which leads to the protection of a sensor network from several attacks, these mechanisms are:q-composite keys scheme, multipath-reinforcement scheme and random-pairwise keys scheme.Also researcher use many metric to evaluate their scheme (resilience against node capture, resistance against node replication, revocation, scale.
  First mechanism is used q common keys, where $q>1$ instead of using a single key as in the basic scheme, where these keys are an overlap to increase the link security in the network, which means that adversary find it difficult to break the link, since this scheme differs from the basic scheme in overlapping keys and reduce the key bool size, key bool size should not be very large and that leads to that the network is not be connected after bootstrapping protocol is complete, nor very small because this leads to sacrifice in network security, where must choose key bool size appropriate. this mechanism is strong for the network when the small number of nodes captured.
  Second mechanism uses the same setup key in basic scheme, difference is to add random values to secure the link between two nodes, such as A and B, where A generates random values with common key along the path, the benefit is even if an eavesdropper discovered key from the other nodes, it cannot penetrate the path, also eavesdropper needs discovery full path values even eavesdropper can penetrate the path.This mechanism cannot operate with the mechanism of q-composite together because it leads to a security weakness.

Third mechanism is as follows before the deployment of nodes, each node generates ID for the total of network nodes (n), where each node has ID, pairwise key. After deployment, setup key phase begins where each node broadcasts ID to its neighbors, any node knows ID, and this node can use the authentication through pairwise and accomplish handshake operation.  Because of the small size of ID, which gives less traffic and less computations this leads to expansion of the network through the rebroadcast on the level of hop but brings with it the problem of DOS attack. this mechanism has many advantages that is	perfect resilience against node capture, node-to-node identity authentication, Distributed Node Revocation(Not to rely on BS to revoke nodes), resistance to node replication and generation and comparable scalability},
  organization={IEEE}
}       

@article{p2,
  title={Design, analysis and performance evaluation of group key establishment in wireless sensor networks},
  author={Chatzigiannakis, Ioannis and Konstantinou, Elisavet and Liagkou, Vasiliki and Spirakis, Paul},
  journal={Electronic Notes in Theoretical Computer Science},
  volume={171},
  number={1},
  pages={17--31},
  year={2007},
  abstract={In this paper, a new Group Key Establishment protocol proposed in wireless sensor network restricted energy, where the aim of this research is to protect the network with the energy balance. The authors noted that the best protocol maintains the energy and uses memory requirements limited and suitable for networks wireless sensor is GDH.3, the researchers had improved this Protocol where the nodes in this protocol have the tasks of unequal, which last node (Mn) implements more tasks than other nodes. This leads to consumption capacity quickly in sensors. GDH.3 protocol uses four stages, the new protocol cancelled a stage 3 and 4, where it make all nodes implement the same tasks that led to the energy balance. The proposed protocol applied in real networks and simulation. The results showed that GDH.3 better than the new protocol to work in small networks, while the new protocol better to work in large networks. The new protocol needs more time than the GDH.3 protocol because the new protocol needs many encryption and decryption operations but in return the new protocol is the best in the energy balance in the sensors network, and the results showed that the change in the spacing nodes does not affect the energy of nodes in the new protocol while large spacing between nodes affect greatly on the energy consumption in GDH.3 protocol},
  publisher={Elsevier}
}

@inproceedings{p3,
  title={Elliptic curve cryptography in practice},
  author={Bos, Joppe W and Halderman, J Alex and Heninger, Nadia and Moore, Jonathan and Naehrig, Michael and Wustrow, Eric},
  booktitle={International Conference on Financial Cryptography and Data Security},
  pages={157--175},
  year={2014},
  abstract={This paper focus on the weak points of ECC in practice through the study of four SSH, TLS, Bitcoin and the Austrian citizen card protocols. The researchers discovered that the ECC to suffer from the weakness of security such as former security systems. They are focused on the three points is the deployment, weak keys, vulnerable signatures. They gathered data bases belonging to the four protocols and found that there are several cases of repeated public keys in SSH and TLS. Bitcoins suffer that there are several signatures share nonces that allow attacker to compute the corresponding private keys and steal coins. Australian citizen card as well as suffer from a multi-use the keys. The big problem with encryption is not random adequacy in generating keys or nonces in digital signatures, for example, Debian OpenSSL},
  organization={Springer}
}

@inproceedings{p4,
  title={A key-management scheme for distributed sensor networks},
  author={Eschenauer, Laurent and Gligor, Virgil D},
  booktitle={Proceedings of the 9th ACM conference on Computer and communications security},
  pages={41--47},
  year={2002},
  abstract={In this research, the researchers designed new key management scheme for DSN networks large scale. This scheme depends on the possibility of the sharing of the key among nodes in random graph. In this scheme, there are three stages are before the distribution, shared-key discovery and path-key establishment. The first stage summed up in several steps: Configure key pool with the identifiers of the key, create a key ring from the key pool, load key ring to each node and link node ID with a trusted controller node. Then begins the second stage of which is that each node broadcast identifiers, any other node has the same node identifier, and this node can connect to and use the shared-key. Then the third phase begins, which is that nodes that do not use the shared-key is possible to use the path-key, where this node connects through two links or more at the end of the key discovery phase. This scheme also provides two mechanisms revocation and rekeying to protect the key. In this scheme cannot be compromised all links like the previous traditional mechanisms because if the attacker compromised node, he cannot access all the links because it does not have all the keys},
  organization={ACM}
}

