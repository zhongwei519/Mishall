@article{p19,
  title={Elliptic curve cryptography-based access control in sensor networks},
  author={Wang, Haodong and Sheng, Bo and Li, Qun},
  journal={International Journal of Security and Networks},
  volume={1},
  number={3-4},
  pages={127--137},
  year={2006},
  abstract={In this paper, ECC algorithm implemented with access control schemes, where the implementation of this algorithm to the sensor node TelosB and compared this scheme with two different schemes (ECCM and TinyECC) on the same sensor node TelosB, through experiments proved that this scheme is superior to other schemes. Because of the processor constrained-sources, the authors focused on the ability of computation and on the use of the domain finite field GF (P). They used large integers method in modular multiplication, and used three mathematical operations are multiplication, division and reduction, these operations are crucial in performance. They also noted that PADD take more cost than PDBL. In tests this scheme relied on several measures is the execution time, power consumption and size of the implementation code, and compare these measurements with ECCM and TinyECC schemes. Sliding window method is used with this scheme (window size = 4), this method is fast but require memory. Researchers have proven that this scheme is better than the other schemes in the FPM and RPM and PADD and PDBL in terms of execution time. As they compared this scheme (ECDSA) and Tinyecc and proved the effectiveness of their scheme through the results. Also Authors have made another comparison between this scheme with a symmetric encryption for access control scheme, they noted that symmetric encryption is more efficient but suffers from several problems, such as scalability, storage, pre-deployment and key distribution. In the end, the authors concluded Applicability (public key algorithms) ECC in WSN, as recent studies have shown that the point multiplication in the ECC can take less than a second},
  publisher={Inderscience Publishers}
}

@article{p20,
  title={Implementing the ECDSA on Different Coordinate Systems},
  author={Chefranov, G},
  journal={American Scientific Research Journal for Engineering, Technology, and Sciences (ASRJETS)},
  volume={1},
  number={1},
  pages={10--16},
  abstract={This paper showed the problem of the high computation cost in the ECDSA algorithm because operations intensity (Adding, Doubling). Author tested many coordinate systems to increase the speed of implementation in addition and doubling operations. The author has concluded that better coordinate system is a mixed coordinate system, which consumes the least amount of time to generation and signature verification of the other coordinate systems},
  year={2013}
}

@inproceedings{p21,
  title={A New Secure Authentication Scheme Based Threshold ECDSA For Wireless Sensor Network.},
  author={Wang, Hao and Wu, Zhongfu and Tan, Xin},
  booktitle={Security and Management},
  pages={129--133},
  abstract={This paper used authentication method depending on servers CAs, where authors added ID and ECDSA threshold and trust value with the public key to authorize node to gain access to the information, which means that this scheme provides integration, authorization and non-repudiation},
  year={2006}
}

@inproceedings{p22,
  title={The Security of DSA and ECDSA},
  author={Vaudenay, Serge},
  booktitle={International Workshop on Public Key Cryptography},
  pages={309--323},
  year={2003},
  abstract={This paper presented a study in DSA and ECDSA algorithms in detail and discussed weaknesses in both algorithms. Author explained that these algorithms become unsuitable for signing messages (integration) if used poorly, as this paper has focused on the parameters validation of security to ensure a strong security for these algorithms against different attacks, also they proved that these algorithms become strong If the parameters are well validation. Author gave its recommendations to update the validation scheme by seed (and thus q with the field representation) in the generator and by generating an additional bit},
  organization={Springer}
}

@inproceedings{p23,
  title={Hardware implementation of elliptic curve digital signature algorithm (ECDSA) on Koblitz curves},
  author={Nabil, Ghanmy and Naziha, Khlif and Lamia, Fourati and Lotfi, Kamoun},
  booktitle={Communication Systems, Networks \& Digital Signal Processing (CSNDSP), 2012 8th International Symposium on},
  pages={1--6},
  year={2012},
  abstract={This paper has focused on reducing the time and increase performance in ECDSA algorithm through exchange doubling point with frobenius scheme in the point multiplication. This paper uses W7 algorithm to increase random integer k, where that the W7 generator better than other generators in the performance and area. ECDSA algorithm used curves of subfields Koblitz with key length a 163-bit. It also carried out on a hardware platform (xilinx) with the hardware description language VHDL. This scheme got good results in performance over time where key generation took time 0.2 ms, signature generation took 0.8 ms and signature verification took 0.4 ms. The authors explained that their scheme has the optimization where it can be used in constrained-source devices such as smart cards, WSN and RFID},
  organization={IEEE}
}

@inproceedings{p24,
  title={Analysis of ecdsa authentication processing in vanets},
  author={Petit, Jonathan},
  booktitle={2009 3rd International Conference on New Technologies, Mobility and Security},
  pages={1--5},
  year={2009},
  abstract={This research investigated the use of ECDSA algorithm in VANET with keys length 224-bit and 256-bit. ECDSA algorithm have high costs in terms of communication and computation. Therefore, researcher discussed the time complexity in the scalar multiplication, modular multiplication, modular inversion in the ECDSA algorithm, in addition to analysis the time complexity and authentication process on the braking distance. Also, this scheme used Montgomery algorithm to the point multiplication and explained that it offers better performance than other algorithms. The researcher explained that the point multiplication consumes more time to generate and verify the signature},
  organization={IEEE}
}

@inproceedings{p25,
  title={A Novel Algorithm for Scalar Multiplication in ECDSA},
  author={Li, Hui and Zhang, Ruixia and Yi, Junkai and Lv, Hongqiang},
  booktitle={Computational and Information Sciences (ICCIS), 2013 Fifth International Conference on},
  pages={943--946},
  year={2013},
  abstract={This paper focused on improving the ECDSA algorithm by improving scalar multiplication. Authors proposed new scheme to improve scalar multiplication by reducing point addition. They presented a method to generate K depend on the generation of an integer S periodically. They got good results in improving point addition $\sqrt(3/4 XL)$ while previous scheme (L/2), new scheme does not require additional memory. Point double in new scheme is the same as previous schemes (D \& A, NAF, WM). The new algorithm depends on the growth rate of a small with bit length growth in scalar(k). also it is appropriate to implement in H/W because it needs simple operands},
  organization={IEEE}
}
@inproceedings{p26,
  title={Enhancement security level and hardware implementation of ECDSA},
  author={Ghanmy, Nabil and Fourati, Lamia CHaari and Kamoun, Lotfi},
  booktitle={2013 IEEE Symposium on Computers and Communications (ISCC)},
  pages={000423--000429},
  year={2013},
  abstract={This paper discuses increasing security level in ECDSA algorithm through using $F2^m$ and Montgomery algorithm ( point multiplication). The authors use key size with length 233 bit for private and public keys, and SHA224 algorithm to increase security level. They explain that $F2^m$ is faster than Fp while $F_p$ is more security. Therefore, authors increased key size to gain better performance and security at the same time. This paper compered results it with the other schemes in terms performance and security, and appropriate to devices constrained-source. Also they conclude that p256- SHA224 is better security than $F2^{233}$},
  organization={IEEE}
}
@inproceedings{p27,
  title={Side channel attack on multiprecision multiplier used in protected ECDSA implementation},
  author={Varchola, Michal and Drutarovsky, Milos and Repka, Marek and Zajac, Pavol},
  booktitle={2015 International Conference on ReConFigurable Computing and FPGAs (ReConFig)},
  pages={1--6},
  year={2015},
  abstract={This paper focused on analysis of CPA attack on ECDSA algorithm in FPGA. Authors did not use traditional power models (Hamming weight/distance) in analysis of CPA attack, but used a new type as countermeasure (chosen plaintext attack). They prove possible successful feasible CPA in ECDSA on FPGA in case $k^{-1}(e+rd_A)$ or $k^{-1}e+(k^{-1}d_A)r$, but also CPA attack had not impact in ECDSA in case using equation $k^{-1}e+(k^{-1}r)d_A$. Authors implement this attack and got results through DISIPA platform},
  organization={IEEE}
}
@article{p28,
  title={Low-Latency ECDSA Signature Verification--A Road Toward Safer Traffic},
  author={Kne{\v{z}}evi{\'c}, Miroslav and Nikov, Ventzislav and Rombouts, Peter},
  year={2014},
  abstract={This paper focus on low-latency in signature verification (ECDSA). ITS suffers from large load through signature verification (such as C2C or C2I) that reach 1000 message to verify signatures per second because there is latency in verification. Authors proposed using ASIC to reduce latency, where they use ECDSA with NIST p-256 curve. They focus on low-latency more than throughout because low-latency is the most important in ITS. Through results, this scheme performs more than 27000 signature verification per second and efficiency 24.5 SGE, and better than previous schemes. This scheme is convenient to security and performance in C2C or C2I over ITS},
  publisher={IEEE}
}

@inproceedings{p29,
  title={Comparison of innovative signature algorithms for WSNs},
  author={Driessen, Benedikt and Poschmann, Axel and Paar, Christof},
  booktitle={Proceedings of the first ACM conference on Wireless network security},
  pages={30--35},
  year={2008},
  abstract={This paper compares many different signature schemes (ECDSA, XTR-DSA and NTRUSing) in terms energy consumption, memory, keys length and signature, and performance. XTR-DSA is fast because it compresses signature NTRUSing uses one core operation and this leads to increase computation fast more than other schemes (ECDSA and XTR-DSA). Through implementation, authors found that NTRUSing algorithms is the best in term performance and memory, where this algorithm uses ROM=11.3 KB and RAM=542 B, as it generates signature in 619 ms and verifies signature in 78 ms. NTRUSing algorithms uffer from security weakness against attacks. Authors implement these algorithms on Micaz mote},
  organization={ACM}
}

@inproceedings{p30,
  title={Message authentication in vehicular ad hoc networks: Ecdsa based approach},
  author={Manvi, SS and Kakkasageri, MS and Adiga, DG},
  booktitle={Future Computer and Communication, 2009. ICFCC 2009. International Conference on},
  pages={16--20},
  year={2009},
  abstract={This paper uses ECDSA algorithm to authenticate message with VANET. Authors use C language to design ECDSA algorithm (generation key, signature and verification). They note through result generation key, signature and verification delay increase with an increase in curve size. This scheme provide integrity, authentication and non-repudiation, as it is convenient constrained-source networks(VANET)},
  organization={IEEE}
}

@inproceedings{p31,
  title={Short hash-based signatures for wireless sensor networks},
  author={Dahmen, Erik and Krau{\ss}, Christoph},
  booktitle={International Conference on Cryptology and Network Security},
  pages={463--476},
  year={2009},
  abstract={This paper proposes a hash-based signature scheme. This scheme depends on modification of Winternitz one time to produce sizes of small signatures and hash chain one time to verify keys with modified through using Merkle Tree scheme (to specific signatures number in each one key pair). This scheme uses hash chain traversal scheme to avoid storage problem all keys pairs. Authors use all these algorithms to increase security in this scheme in hostile environments. Through result authors compared this scheme (symmetric cryptographic AES with 80 bit) with ECDSA ($F_p$ with 160 bit) in term signature generation, verification and timing. This scheme generates 7 times faster and verification158 times faster than ECDSA. Authors notes through results that time of signature generation is the same as ECDSA, while time of signature verification is 20 times faster than ECDSA. They note if message length is larger than 14 bit, ECDSA becomes more efficient than this scheme, as private keys in this scheme is larger than private key in ECDSA},
  organization={Springer}
}

@inproceedings{p32,
  title={Efficient implementation of public key cryptosystems on mote sensors (short paper)},
  author={Wang, Haodong and Li, Qun},
  booktitle={International Conference on Information and Communications Security},
  pages={519--528},
  year={2006},
  abstract={This paper presents implementation for two algorithms (1024-bit RSA and 160-bit ECC) on sensor node MICAz. It investigates using these algorithms on constrained-source devices. Authors uses many efficient schemes to improve performance for algorithms (RSA and ECC). They use hybrid multiplication to reduce access memory, where they note efficient of this method better than multi-precision multiplication. They use two schemes in RSA algorithm are Montgomery reduction and CTR to reduce cost of modular reduction. Also they use mixed coordinate in ECC algorithm to improve efficient for multiplication and squaring and use window method to improve point multiplication instead of NAF method. They get results for RSA on MICAz, where public key =0.79s and private key= 21.5s. ECC results on MICAz is signature generation=1.3s and signature verification = 2.8s. To purpose comparison, authors implemented ECC on TelosB, where MICAz results slightly less than TelosB results. Authors prove using RSA and ECC on WSN},
  organization={Springer}
}

@inproceedings{p33,
  title={Exploring the design space of prime field vs. binary field ECC-hardware implementations},
  author={Wenger, Erich and Hutter, Michael},
  booktitle={Nordic Conference on Secure IT Systems},
  pages={256--271},
  year={2011},
  abstract={This paper presents comparison in two finite field ($F_{p192}$ and $F2^{191}$) to ECC Hardware implementations. Authors use multi-precision multiplication in finite field arithmetic and using set of measures for comparison between $F_p$ and $F2^m$ are area, speed, power and energy. They use these metrics to compare $F_p$ and $F2^m$ in point multiplication and ECDSA (signature generation and signature verification). Through results, authors found that $F2^m$ is better than $F_p$ through 19.7% in area, 69.6% in speed, 15.9% in power and 74.4% in energy (in point multiplication). In the case of the use ECDSA (sig. and ver.), authors found $F_p$ is better in area and power through using Vanish processor than $F_p$, but still $F2^m$ is better than $F_p$ in energy and execution time. They compared their results with previous studies, they found that their result in $F_p$ is better area and energy than previous studies, in the case $F2^m$ is better only in area than previous studies (where previous studies use curve $F2^{163}$). Also authors introduce explain in details for finite field arithmetic (Addition, subtraction, multiplication, squaring and inversion) and explain why $F2^m$ faster than $F_p$, where F2m uses simpler operations such as XOR than $F_p$},
  organization={Springer}
}

@inproceedings{p34,
  title={Simple hash based message authentication scheme for wireless sensor networks},
  author={Abduvaliev, Abror and Lee, Sungyoung and Lee, Young-Koo},
  booktitle={Communications and Information Technology, 2009. ISCIT 2009. 9th International Symposium on},
  pages={982--986},
  year={2009},
  abstract={This paper presents a new proposed to modify SHA-1 algorithm and using it with pre-shared secret key (ECDH). Modifying in ShA1 algorithm (mSHA-1) is through exchange logic functions to pseudo-random function plus secret key $k_s$ ($F(w_i)=w_i* \sqrt(2) *k_s$). This paper aims to reduce computation and communication in WSN, authors use mShA-1 with ECDH to reduce cost of computation and communication. They use two scenarios for WSN (small-scale and large-scale (clustering)). Results prove this scheme is better than previous works in security and performance},
  organization={IEEE}
}


@inproceedings{p35,
  title={A Fault Attack on ECDSA},
  author={Schmidt, J{\"o}rn-Marc and Medwed, Marcel},
  booktitle={Fault Diagnosis and Tolerance in Cryptography (FDTC), 2009 Workshop on},
  pages={93--99},
  year={2009},
  abstract={This paper presents a new attack (fault attack) on ECDSA, Attacker uses this attack to modify program flow and tries to benefit from error result to get on parts (bits) for ephemeral key k, then performing lattice attack to obtain on private key (d). Authors implement this attack on point multiplication through Montgomery ladder and D\&A algorithm. The successful in this attack is probability, this attack tries to get on bits from k through many signatures. Authors use projective Jacobian coordinate system through implementation because it is efficient. Also authors use countermeasure to fault attack through using discrete algorithm l in point representation (Q=(X:Y:Z;l), where modifying by adversary for point addition and point doubling is detection. This scheme can prevent fault and SCA (with random mask) attack. Also this paper present comparison between this scheme and other schemes},
  organization={IEEE}
}


@inproceedings{p36,
  title={A public-key infrastructure for key distribution in TinyOS based on elliptic curve cryptography},
  author={Malan, David J and Welsh, Matt and Smith, Michael D},
  booktitle={Sensor and Ad Hoc Communications and Networks, 2004. IEEE SECON 2004. 2004 First Annual IEEE Communications Society Conference on},
  pages={71--80},
  year={2004},
  abstract={Authors implement Ecc algorithm in TinyOS on MICA 2 and use PKI to distribute keys. They use binary field $F2^p$ with polynomial base because it is efficient in software. They use two implementations (EccM 1.0 and EccM 2.0), where EccM 1.0 fails when key size becomes 63 bit, therefore they found that EccM 2.0 is more efficient than EccM 1.0 because EccM 1.0 uses random curve while EccM 2.0 uses curves recommended by NIST. Through result, time of keys generation (private and public) is 34.161 s through 100 trial and time of shared keys generation is 34.173 s through 100 trial. This paper refers to success feasible public key on WSN (MICA 2), also authors explain that TinyOS performs security computation and efficient among sensors. They showed that PKI presents efficient security in cost of low memory to distribute keys on ECC (TinyOS on MICA 2).},
