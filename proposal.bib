}

@inproceedings{p77,
  title={Curved Tags--A Low-Resource ECDSA Implementation Tailored for RFID},
  author={Pessl, Peter and Hutter, Michael},
  booktitle={International Workshop on Radio Frequency Identification: Security and Privacy Issues},
  pages={156--172},
  year={2014},
  abstract={This paper presented implementation of ECDSA algorithm with prime field ($F_{p160}$) on RFID. Authors exchange SHA-1 hash algorithm with KECCAK hash to reduce running time in ECDSA on RFID. Also they used fixed-base comb with w=4 to accelerate and reduce hardware requirements. Through results, their scheme gets area 12448 GEs, power consumption 42.42 $\mu w$ in 1MHz and generate signature is less than 140 kcycles. With these results, their scheme competes binary and prime field schemes. Authors used countermeasures to prevent implementation attacks. They used zeroless signed digit (ZSD) with comb method to prevent simple power analysis (SPA) and used randomized projective coordinate to prevent differential power analysis (DPA) and template based SPA attack},
  organization={Springer}
}

@inproceedings{p78,
  title={An ECDSA processor for RFID authentication},
  author={Hutter, Michael and Feldhofer, Martin and Plos, Thomas},
  booktitle={International Workshop on Radio Frequency Identification: Security and Privacy Issues},
  pages={189--202},
  year={2010},
  abstract={This paper proposed ECDSA processor with RFID tag. This processor has security services (authentication, integrity and non-repudiation). Authors used this processor to authenticate between tag reader and RFID tag. They used Montgomery ladder algorithm to improve PM's performance and countermeasure against SPA attacks. They used SHA-1 algorithm to generate ephemeral $k$ and to gain appropriate randomness as countermeasure against side channel attacks, where generation of random $k$ according to FIPS's standard. Also they used randomized Projective coordinate as countermeasure against side channel attacks. In signature algorithm, they used equation $s=k^-1e+(k^-1r)d$ instead of $s=k^-1(e+rd)$ to prevent DPA attacks. Through results, computation cost of signature in this processor is 859188 clock cycles (127ms at 6.78 MHz) and area is 19115 GE},
  organization={Springer}
}

@article{p79,
  title={Two is the fastest prime: lambda coordinates for binary elliptic curves},
  author={Oliveira, Thomaz and L{\'o}pez, Julio and Aranha, Diego F and Rodr{\'\i}guez-Henr{\'\i}quez, Francisco},
  journal={Journal of Cryptographic Engineering},
  volume={4},
  number={1},
  pages={3--17},
  year={2014},
  abstract={This paper presented $\lambda$-Projective coordinate through using three coordinates $(X,Y,Z)$ with binary field $(F_{2^{254}})$ to improve computation efficiency in PM. Formula used for coordinates is $(X/Z,L/Z)$ corresponding for $\lambda$-Affine formula, where $\lambda$-Affine formula is $(X,X+Y/X)$. $\lambda$-Projective improved LD-Projective, where D\&A cost in $\lambda$-Projective is 11m+2s while in LD-Projective is 13m+4s. Also $2P+Q$ cost in $\lambda$-Projective is 10m+6s while in LD-Projective is 11m+10s. In addition, $\lambda$-Projective improved H\&A to 60\% in squaring and 6\% in multiplication better than LD-Projective. Their scheme implemented on Sandy Bridge platform. Authors get computation 69500 clock cycles through single core with H\&A while 47900 clock cycles through multi core with combining Gallant, Lambert and Vanstone,(GLV) technique, H\&A and D\&A in SM unprotected. Also they get computation 114800 clock cycles through singe core protected. Their scheme gets performance better 2\% than Ivy Bridge and 46\% than Haswell platforms},
  publisher={Springer}
}

@inproceedings{p80,
  title={Non-interactive deniable authentication protocol using generalized ecdsa signature scheme},
