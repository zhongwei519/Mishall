@inproceedings{p74,
  title={Low-resource ECDSA implementation for passive RFID tags},
  author={Kern, Thomas and Feldhofer, Martin},
  booktitle={Electronics, Circuits, and Systems (ICECS), 2010 17th IEEE International Conference on},
  pages={1236--1239},
  year={2010},
  abstract={This paper presented hardware's implementation for ECDSA algorithm on RFID technology. Authors used ECDSA algorithm to authenticate entity. They are applied this algorithm with prime field ($F_{p160}$) according to SECG standards. They accelerate multiplication algorithm through combining integer multiplication and fast reduction. Through implementation, they get good results in reducing chip area and power consumption. They conclude that ECDSA computation requires 511864 clock cycles and power consumption requires $860\mu w$ in 1MHz},
