  author={Medwed, Marcel and Oswald, Elisabeth},
  booktitle={International Workshop on Information Security Applications},
  pages={14--27},
  year={2008},
  abstract={This paper investigated implementation of template based SPA attack in ECDSA with microprocessor 32-bit (ARM7 architecture). Authors point out that this attack is applicable on ECDSA through three basics to combine with their attack (few bits to retrieve $k$ by lattice attack, using fixed base point and microprocessors enough to build templates). They used double-and-always-add algorithm to prevent SPA attacks. They described that countermeasure to prevent this attack is using randomness coordinates in DPA resistance},
  organization={Springer}
}

@inproceedings{p83,
  title={Preventing SPA/DPA in ECC systems using the Jacobi form},
  author={Liardet, P-Y and Smart, Nigel P},
  booktitle={International Workshop on Cryptographic Hardware and Embedded Systems},
  pages={391--401},
  year={2001},
  abstract={This paper proposed using Jacobi form instead of Weierstrass to prevent SCA (SPA and DPA) attacks. This form allows for addition formula operations (addition and doubling) to have identical time and power and this leads to prevent SPA and DPA attacks. Authors also used second countermeasure against SPA and DPA that combining point blinding and randomized signed window method. They pointed out that their scheme less efficient 70\% than original scheme (Weierstrass). Where average of field operations in their scheme are 3664 while in original scheme are 2136. But this scheme is better performance than original scheme in the case countermeasures are not applied against SPA and DPA attacks},
  organization={Springer}
}

@inproceedings{p84,
