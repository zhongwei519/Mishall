  booktitle={International Workshop on Public Key Cryptography},
  pages={28--40},
  year={2004},
  abstract={This paper presents a method to use point halving with frobenius endomorphism ($\tau$-adic). Authors use point halving because it is faster three times than point doubling, thereby they use $\tau$-and-add instead of double-and-add with Koblitz curves, these curves have useful features in speeding kp. They use these curves with fields sizes (k-163 bit and k-233 bit) to standards of NIST's FIPS in binary fields. $\tau$-NAF used to reduce point addition from n/3 to 2n/7. Results prove that $\tau$-and-add based $\tau$-NAF provides speeding 14.28\% better than frobenius method in addition to it does not need requirements of additional memory in the case using normal basis},
  organization={Springer}
}

@article{p57,
  title={Field inversion and point halving revisited},
  author={Fong, Kenny and Hankerson, Darrel and L{\'o}pez, Julio and Menezes, Alfred},
  journal={IEEE Transactions on Computers},
  volume={53},
  number={8},
  pages={1047--1059},
  year={2004},
  abstract={This paper present analysis and comparison between SM methods (point doubling and point halving), implementations presented in software platform and using binary fields ($F2^{163} and F2^{233}$) and reduction polynomials of trinomial and pentanomial (that is using polynomial basis instead of normal) on standards of NIST's FIPS 186-2. Through analysis, authors found that point halving is faster (29\%) than point doubling when p is known in advance. Also they note $\tau$-adic (used in \cite{p56}) faster than PH. They present comparison between double-and-add and halve-and-add over $F2^{163}$ through affine and projective coordinate systems. They point out that point doubling can used with mixed coordinate while point halving must use in affine coordinate. As they explain that signature verification in ECDSA has 2 SM and this operation is expensive when P is known but performing ECDSA verification with halving is more efficient than doubling in addition to halving is better for constrained-resources in the case assumption that storage is available},
  publisher={IEEE}
}


@inproceedings{p58,
  title={Improved fixed-base comb method for fast scalar multiplication},
  author={Mohamed, Nashwa AF and Hashim, Mohsin HA and Hutter, Michael},
  booktitle={International Conference on Cryptology in Africa},
  pages={342--359},
  year={2012},
  abstract={This paper presented a method to improve PM through combing comb method with width-w NAF and that it to reduce point additions. Comb method depends on precomputation for points to improve PM performance. Improving method presented the best results than original comb and NAF with comb. Also, this scheme used to accelerate multiple PM (kP +rQ) in ECDSA algorithm. This scheme is not resistant for SCA attacks; therefore, authors suggested using constant runtime to prevent SPA and timing (through add point of infinity (O) when bits value equal nonzero and also add it to precomputation values). In addition, they recommended using randomness to prevent DPA attacks. Authors explain their algorithm convenient for devices constrained-source (memory) when choice suited parameters for algorithm such as window size. This method improved computation complexity from 33\% to 38\% compared to NAF with comb in devices constrained-sources},
  organization={Springer}
}

@inproceedings{p59,
